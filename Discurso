      "cell_type": "code",
      "source": [
        "import random\n",
        "\n",
        "palabras_reggaeton = {}\n",
        "\n",
        "def generador_cancion(palabras_reggaeton):\n",
        "    cancion = ''\n",
        "    for key in palabras_reggaeton.keys():\n",
        "        word = random.choice(palabras_reggaeton[key])\n",
        "        cancion += word\n",
        "    return cancion\n",
        "\n",
        "sujeto = ['Mami ', 'Bebé ', 'Princes ', 'Mami ', 'Brrr ', 'Reina ', 'Hermosa ']\n",
        "intencion = ['yo quiero ', 'yo puedo ', 'yo vengo a ', 'voy a ', 'pretendo ', 'me gustaría ']\n",
        "accion = ['encendelte ', 'amalte ', 'ligar ', 'jugar ', 'cocinar ', 'trabajar ']\n",
        "adjetivo = ['suave ', 'lento ', 'rápido ', 'fuerte ', 'duro ', 'convencido ']\n",
        "duracion = ['hasta que salga el sol ', 'toda la noche ', 'hasta el amanecer ', 'todo el dia ', 'por la noche ']\n",
        "manera = ['sin anestecia.', 'sin compromiso.', 'feis to feis.', 'sin miedo.', 'intensamente.']\n",
        "\n",
        "palabras_reggaeton['sujto'] = sujeto\n",
        "palabras_reggaeton['intencion'] = intencion\n",
        "palabras_reggaeton['accion'] = accion\n",
        "palabras_reggaeton['adjetivo'] = adjetivo\n",
        "palabras_reggaeton['duracion'] = duracion\n",
        "palabras_reggaeton['manera'] = manera\n",
        "\n",
        "cancion = generador_cancion(palabras_reggaeton)\n",
        "print(cancion)"
      ],
